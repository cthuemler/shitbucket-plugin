{
  "manifest_version": 3,
  "name": "Shitbucket",
  "version": "1.0",
  "description": "This plugin does absolutely nothing except turn the Bitbucket logo into a shitbucket.",
  "icons": {
    "48": "icons/new-logo.svg"
  },
  "content_scripts": [
    {
      "matches": ["*://*.bitbucket.org/*"],
      "js": ["shitbucket.js"],
      "css": ["shitbucket.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/new-logo.svg"],
      "matches": ["*://*.bitbucket.org/*"]
    }
  ]
}
